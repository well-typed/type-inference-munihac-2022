build-type: Simple

executable infer
  build-depends:
    base,
    containers,
    megaparsec,
    mtl,
    haskeline
  main-is: Infer.hs
  other-modules:
    Parser
    Repl
    Syntax
    Types
  other-extensions:
    FlexibleInstances
    GeneralizedNewtypeDeriving
    InstanceSigs
    TypeSynonymInstances
    TupleSections
  ghc-options: -Wall
  default-language: Haskell2010
  hs-source-dirs: src
